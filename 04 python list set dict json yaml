import sys
from ruamel.yaml import YAML
from ruamel.yaml.comments import CommentedMap as ordereddict

data = {'@': {'%':'%',1:[{1:1,2:2,3:3},['A','B','C']],'#':{'#':{'#':'#'}}}, 'å£':['æ›°','ç”°']}
yml = YAML()
yml.indent(mapping=6, sequence=(20), offset=(20-2))         ## {}=mapping,[]=sequence,offset = '-'(å•åˆ—ç¬¦)
yml.dump(data, sys.stdout)

